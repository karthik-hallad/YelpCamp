<% layout("../layouts/boilerplate") %>

  <div class="alert alert-danger m-4" role="alert">
    <h4 class="alert-heading">Error!</h4>
    <p>
      <%= message %>
        <br>
        <br>
        <% if(process.env.NODE_ENV !== 'production') { %>
        <%= err.stack %>
        <% } %>
    </p>
    <hr>
    <p class="mb-0">Status Code: <%= status %>
    </p>
  </div>